if Games[game.PlaceId] then
    loadstring(game:HttpGet(Games[game.PlaceId]))()
    Rayfield:Destroy() 
else
Rayfield:Notify({
	Title = "Perfectus Hub",
	Content = "Game is Not Supported",
	Duration = 6.5,
	Image = 14887567417,
	Actions = { -- Notification Buttons
		Ignore = {
			Name = "Okay!",
			Callback = function()
				print("The user tapped Okay!")
			end
		},
	},
})
end
