    [13916037343] = "https://raw.githubusercontent.com/PZYXF008974/SSURFETIC88965/main/5", -- Kick Door Simulator
    [13681795330] = "https://raw.githubusercontent.com/PZYXF008974/SSURFETIC88965/main/7", -- Push Simulator
    [12996550309] = "https://raw.githubusercontent.com/PZYXF008974/SSURFETIC88965/main/9", -- RunStar Race
    [9285238704] = "https://raw.githubusercontent.com/PZYXF008974/SSURFETIC88965/main/8", -- Race Clicker
    [13370783664] = "https://raw.githubusercontent.com/PZYXF008974/SSURFETIC88965/main/3", -- Anime Energy
    [14433762945] = "https://raw.githubusercontent.com/PZYXF008974/SSURFETIC88965/main/11", --Anime Champ Sim
